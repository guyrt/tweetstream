{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "LogWriterStatement2015-10-31",
            "Effect": "Allow",
            "Action": [
                "logs:FilterLogEvents",
                "logs:GetLogEvents",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:us-west-2:984616268605:log-group:tweetstream:log-stream:*"
            ]
        },
        {
